,QAAD,EAAc;AAClB,UAAM,MAAK,GAAI,QAAQ,CAAC,IAAxB;AACA,UAAM,GAAE,GAAI,MAAM,CAAC,IAAP,CAAY,UAAC,IAAD;AAAA,eAAU,IAAI,CAAC,GAAL,KAAa,KAAvB;AAAA,OAAZ,CAAZ;AACA,MAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,GAArB;AACA,UAAM,GAAE,GAAI,MAAM,CAAC,IAAP,CAAY,UAAC,IAAD;AAAA,eAAU,IAAI,CAAC,GAAL,KAAa,KAAvB;AAAA,OAAZ,CAAZ;AACA,MAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,GAArB;AACA,UAAM,GAAE,GAAI,MAAM,CAAC,IAAP,CAAY,UAAC,IAAD;AAAA,eAAU,IAAI,CAAC,GAAL,KAAa,KAAvB;AAAA,OAAZ,CAAZ;AACA,MAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,GAArB;AACA,UAAM,GAAE,GAAI,MAAM,CAAC,IAAP,CAAY,UAAC,IAAD;AAAA,eAAU,IAAI,CAAC,GAAL,KAAa,KAAvB;AAAA,OAAZ,CAAZ;AACA,MAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,GAArB;AACD,KAXH,EAYG,KAZH,CAYS,UAAC,GAAD,EAAS;AACd,MAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACD,KAdH;AAeD,GAnCY;AAoCb,EAAA,OAAO,EAAE;AACP,IAAA,IADO,kBACA;AACL,UAAM,MAAK,GAAK,KAAK,IAAL,CAAU,MAAV,GAAmB,KAAK,IAAzB,GAAiC,KAAK,IAArD;AACA,WAAK,IAAL,CAAU,MAAV,GAAmB,MAAM,CAAC,OAAP,CAAe,CAAf,CAAnB;AACD,KAJM;AAKP,IAAA,QALO,oBAKE,KALF,EAKS;AACd,WAAK,IAAL,GAAY,KAAZ;AACD,KAPM;AAQP,IAAA,SARO,qBAQG,KARH,EAQU;AACf,WAAK,IAAL,GAAY,KAAZ;AACD;AAVM;AApCI,CAAf","sourcesContent":["<template>\n  <div>\n    <h2>Converter</h2>\n    <div class=\"container convertr-block\">\n      <div class=\"what\">\n        <label class=\"currency-label\">What</label>\n        <select class=\"form-select\" v-model=\"firstValue\" @change=\"onChange(firstValue)\">\n          <option :value=\"uah_value\">UAH</option>\n          <option :value=\"usd_value\">USD</option>\n          <option :value=\"eur_value\" selected>EUR</option>\n          <option :value=\"rur_value\">RUR</option>\n          <option :value=\"bts_value\">BTC</option>\n        </select>\n        <label class=\"currency-label\">To</label>\n        <select class=\"form-select\" v-model=\"fourValue\" @change=\"onChange2(fourValue)\">\n          <option :value=\"uah_value\">UAH</option>\n          <option :value=\"usd_value\" >USD</option>\n          <option :value=\"eur_value\">EUR</option>\n          <option :value=\"rur_value\">RUR</option>\n          <option :value=\"bts_value\">BTC</option>\n        </select>\n        <label class=\"currency-label\">Nu</label>\n        <input class=\"form-control input\" type=\"number\" v-model=\"post.title1\" />\n      </div>\n      <div class=\"tilda\">\n        <button class=\"btn btn-primary btn-lg conver-button\" @click=\"Calc()\">Convert</button>\n      </div>\n      <div class=\"to to-tilda\">\n        RESULT: {{ this.post.result }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'Convereter',\n  data() {\n    return {\n      usd_value: 0,\n      eur_value: 0,\n      rur_value: 0,\n      bts_value: 0,\n      uah_value: 1,\n      errors: [],\n      post: {\n        title1: 1,\n        title2: 0,\n        val1: 0,\n        val2: 0,\n        result: 0,\n      },\n    };\n  },\n  created() {\n    axios.get('https://api.privatbank.ua/p24api/pubinfo?json&exchange&coursid=5')\n      .then((responce) => {\n        const result = responce.data;\n        const usd = result.find((item) => item.ccy === 'USD');\n        this.usd_value = usd.buy;\n        const eur = result.find((item) => item.ccy === 'EUR');\n        this.eur_value = eur.buy;\n        const rur = result.find((item) => item.ccy === 'RUR');\n        this.rur_value = rur.buy;\n        const btc = result.find((item) => item.ccy === 'BTC');\n        this.bts_value = btc.buy;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n  methods: {\n    Calc() {\n      const result = (this.post.title1 / this.val1) * this.val2;\n      this.post.result = result.toFixed(3);\n    },\n    onChange(value) {\n      this.val1 = value;\n    },\n    onChange2(value) {\n      this.val2 = value;\n    },\n  },\n};\n</script>\n\n<style>\n.convertr-block {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.what, .to {\n  display: flex;\n  flex-direction: column;\n}\n.tilda {\n  margin-left: 50px;\n  margin-right: 25px;\n  font-size: 40px;\n}\n\n.to-tilda {\n  margin-left: 25px;\n  margin-right: 25px;\n  font-size: 40px;\n}\n\n.input {\n  margin-top: 10px;\n}\n\n.currency-label {\n  margin-bottom: 10px;\n  margin-top: 10px;\n}\n\n.conver-button {\n  font-weight: 500;\n}\n\n@media (max-width: 600px) {\n  .convertr-block {\n    flex-direction: column;\n  }\n  .to, .what {\n    margin-top: 30px;\n  }\n}\n\n</style>\n"],"sourceRoot":""}]}