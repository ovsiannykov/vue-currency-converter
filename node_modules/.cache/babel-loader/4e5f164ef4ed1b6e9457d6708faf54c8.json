AAAA,OAAT,EAAgC,GAAhD;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,OAAZ,EAAqB,GAArB;AACD;AANM;AApBI,CAAf","sourcesContent":["<template>\n  <li class=\"currency-item card\" v-bind:id=\"value.ccy\" v-for=\"value in stock\" :key=\"value.stock\">\n    <div>\n      <div class=\"name current\">{{value.ccy}}</div>\n    </div>\n    <div>\n      <div class=\"valut card-titile\">BUY</div>\n      <div class=\"name\">{{Math.round(parseFloat(value.buy) * 100) / 100}}</div>\n    </div>\n    <div>\n      <div class=\"valut card-titile\">SALE</div>\n      <div class=\"name\">{{Math.round(parseFloat(value.sale) * 100) / 100}}</div>\n    </div>\n    <div>\n      <button @click=\"Follow(value.ccy, 'need')\" class=\"like-btn btn btn-primary\"> &#10084;</button>\n    </div>\n  </li>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'CurrencyItem',\n  data() {\n    return {\n      stock: [],\n      errors: [],\n    };\n  },\n  created() {\n    axios.get('https://api.privatbank.ua/p24api/pubinfo?json&exchange&coursid=5')\n      .then((responce) => {\n        const result = responce.data;\n        Object.freeze(result);\n        this.stock = result;\n        console.log(this.stock);\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n  methods: {\n    Follow(index) {\n      const id = index;\n      const arr = this.stock;\n      const element = arr.find((el) => el.ccy === id).ccy;\n      console.log(element, arr);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.currency-item {\n  display: flex;\n  width: 350px;\n  flex-direction: row;\n  justify-content: space-between;\n  align-items: center;\n  padding: 20px;\n  margin-bottom: 20px;\n  text-align: center;\n}\n.card-titile {\n  font-size: 10px;\n}\n.current {\n  font-size: 25px;\n  font-weight: 500;\n}\n</style>\n"],"sourceRoot":""}]}