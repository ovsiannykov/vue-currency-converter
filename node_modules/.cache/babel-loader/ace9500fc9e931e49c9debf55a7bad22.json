     .catch((err) => {\n        console.log(err);\n      });\n  },\n  methods: {\n    Follow(index) {\n      const id = index;\n      const arr = this.stock;\n      const element = arr.find((el) => el.ccy === id).ccy;\n      console.log(element, arr);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.currency-item {\n  display: flex;\n  width: 350px;\n  flex-direction: row;\n  justify-content: space-between;\n  align-items: center;\n  padding: 20px;\n  margin-bottom: 20px;\n  text-align: center;\n}\n.card-titile {\n  font-size: 10px;\n}\n.current {\n  font-size: 25px;\n  font-weight: 500;\n}\n</style>\n"],"sourceRoot":""}]}